<%@ val name: Any %>
<%@ val activities: List[String] %>

<div style="margin-top: 30px; background: #ccc; padding: 10px">
<p>The user name is <%= name %></p>
<p>The selected activities are</p>
<table>
    #for (activity <- activities)
        <tr>
            <td><%= activity %></td>
        </tr>
    #end
</table>
</div>